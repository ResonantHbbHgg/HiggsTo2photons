##
doTriggerSelection=1
reRunCiCForData=1

##
leadEtCut=40.0
subleadEtCut=30.0

##
massMin=100.
massMax=180.
nDataBins=160

## Un-tagged categories
nEtaCategories=2
nR9Categories=2
nPtCategories=1

## Di-jet tag
includeVBF=1
nVBFEtaCategories=1
nVHhadEtaCategories=1

leadEtVBFCut=60
subleadEtVBFCut=25

## Hadronic V tag
includeVHhad=0
leadEtVHhadCut=60
subleadEtVHhadCut=25

## Lepton tag
includeVHlep=0

leadEtVHlepCut=45
subleadEtVHlepCut=25

##
