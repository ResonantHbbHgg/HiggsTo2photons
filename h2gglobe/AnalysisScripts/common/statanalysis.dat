doEcorrectionSmear=1
doEcorrectionSyst=0

doEscaleSmear=1
doEscaleSyst=1

doEresolSmear=1
doEresolSyst=1

doPhotonIdEffSyst=1
doPhotonIdEffSmear=1

doVtxEffSyst=1
doVtxEffSmear=1

doTriggerEffSmear=1
doTriggerEffSyst=1

doR9Smear=1
doR9Syst=1

doKFactorSmear=1
doKFactorSyst=1

doInterferenceSmear=1

## Inputs for reweighing
kfacHist=aux/KFactors_AllScales_interpolated.root
efficiencyFile=aux/sig_reweighing_jan16rereco_baseline_v3.root
puMap=pu_weights_map.dat
puHist=aux/2011_0100_73500.weights3.root
puTarget=aux/nov08_rereco.json.68000.pileup.root

## energy scale adjustement and errors
scale_offset_file=energy_scale_offsets.dat
scale_offset_error_file=energy_scale_errors.dat

## additional MC smearing
smearing_file=smearing_sigma_and_errors.dat

## jets
recomputeBetas=0
recorrectJets=1
rerunJetMva=0
recomputeJetWp=1
